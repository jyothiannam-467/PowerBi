Current Year = var currentItem = SELECTEDVALUE('Table'[Groups])

return SWITCH(TRUE(),
    currentItem = "Net Revenues:","",
    currentItem = "..........United States",[United States],
    currentItem = "..........Canada",[Canada],
    currentItem = "..........United Kingdom",[United Kingdom],
    currentItem = "..........Rest of the World",[Rest Of the World],
    currentItem = "Net Revenues Total",[NetRevenueTotal],
