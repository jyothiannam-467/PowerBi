    currentItem = "..........Lab Operating Costs",[Land Operating],
    currentItem = "..........Freight And Distribution",[Freight And Distribution],
    currentItem = "..........Kit Resale",[Kit Resale],
    currentItem = "Total costs Of Goods Sold",[Total Costs Of Goods Sold],
    currentItem = "Gross Profit","",
    currentItem = "..........Gross Margin %",[Gross Profit Margin],
    currentItem = "Operating Expenses:","",
    currentItem = "..........Research And Development",[Research and Development],
    currentItem = "..........Sales,Marketing and Education",[Sales,Marketing And Education],
    currentItem = "..........General And Administrative",[General],
    currentItem = "..........Bad Debt Expense",[Bad Dept Expense],
    currentItem = "..........Amortization",[Amortization],
    currentItem = "Total Operating expenses",[Total Operating Expenses],
    currentItem = "Operating Income (loss)",[Operating Income (loss)],
    currentItem = "Non-Recurring Expenses:","",
    currentItem = "..........(Gain) loss on Sale of Assests",[(Gain)Loss Of Sales],
    currentItem = "..........Non-Recurring Expenses",[No-Recurring Expenses],
    currentItem = "..........Restructuring Costs",[Restructuring Costs],
    currentItem = "Total Non-Recurring Expenses",[TotalNon-Recurring],
