Nos_FCRA_PO = 
VAR Particular1 = SELECTEDVALUE('Table1'[Column1])
VAR Particular2 = SELECTEDVALUE('Table1'[Column3])

RETURN 
    SWITCH(
        TRUE(),
        
        -- Logic for POs Raised
        Particular1 = "POs Raised", [POsRaised_FCRA_PO],

        -- Logic for Receipt of POs (GRN)
        Particular1 = "Receipt of POs (GRN)" && Particular2 = "Closed", [ReceiptClosed_FCRA_PO],
        Particular1 = "Receipt of POs (GRN)" && Particular2 = "Fully Open", [ReceiptFullyOpen_FCRA_PO],
        Particular1 = "Receipt of POs (GRN)" && Particular2 = "Partly Closed", [ReceiptPartlyClosed_FCRA_PO],
        Particular1 = "Receipt of POs (GRN)" && Particular2 = "Partly Open", [ReceiptPartlyOpen_FCRA_PO],
        Particular1 = "Receipt of POs (GRN)" && Particular2 = "Total" ,[ReceiptClosed_FCRA_PO]+[ReceiptFullyOpen_FCRA_PO]+[ReceiptPartlyClosed_FCRA_PO]+[ReceiptPartlyOpen_FCRA_PO],
        
        -- Logic for Invoicing of POs
        Particular1 = "Invoicing of POs" && Particular2 = "Closed", [InvoicedClosed_FCRA_PO],
        Particular1 = "Invoicing of POs" && Particular2 = "Fully Open", [InvoicedFullyOpen_FCRA_PO],
        Particular1 = "Invoicing of POs" && Particular2 = "Partly Closed", [InvoicedPartlyClosed_FCRA_PO],
        Particular1 = "Invoicing of POs" && Particular2 = "Partly Open", [InvoicedPartlyOpen_FCRA_PO],
        Particular1 = "Invoicing of POs" && Particular2 = "Total",[InvoicedClosed_FCRA_PO]+[InvoicedFullyOpen_FCRA_PO]+[InvoicedPartlyClosed_FCRA_PO]+[InvoicedPartlyOpen_FCRA_PO],
        
        -- Default value
        0 
    )
----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

Nos_FCRA = var currentParticular = SELECTEDVALUE('Table'[Column1])

return SWITCH(TRUE(),
    currentParticular = "POs Raised",[POsRaised_FCRA],
 currentParticular = "Receipt of POs (GRN)","",
 currentParticular = "Closed",[ReceiptClosed_FCRA],
 currentParticular = "Fully Open",[ReceiptFullyOpen_FCRA],
 currentParticular = "Partly Closed",[ReceiptPartlyClosed_FCRA],
 currentParticular = "Partly Open",[ReceiptPartlyOpen_FCRA],
 currentParticular = "Total","",
 currentParticular = "Invoicing of Pos","",
currentParticular = "Closed.",[InvoicedClosed_FCRA],
currentParticular = "Fully Open.",[InvoicedFullyOpen_FCRA],
currentParticular = "Partly Closed.",[InvoicedPartlyClosed_FCRA],
currentParticular = "Partly Open.",[InvoicedPartlyOpen_FCRA],
currentParticular = "Total.","",
 
0)

